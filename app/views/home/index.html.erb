
    <header id="top" class="header">
      <div class="text-vertical-center" id="front">
          <h1>FoodMatch</h1>
          <h3>Food Analytics to Keep You Satisfied</h3>
          <br>
          <a href="#about" class="btn btn-dark btn-lg">Find Out More</a>
      </div>
    </header>


    <!-- About -->
    <section id="about" class="about">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <h2>Data Dining</h2>
                    <p class="lead">Having tough time to decide what to eat during the time? We lead you the way!<br>.</p>
                </div>
            </div>
            <!-- /.row -->
        </div>
        <!-- /.container -->
    </section>

    <!-- Services -->
    <!-- The circle icons use Font Awesome's stacked icon classes. For more information, visit http://fontawesome.io/examples/ -->
    <section id="services" class="services bg-primary">
        <div class="container">
            <div class="row text-center">
                <div class="col-lg-10 col-lg-offset-1">
                    <h2>Our Services</h2>
                    <hr class="small">
                    <div class="row">
                        <div class="col-md-3 col-sm-6">
                            <div class="service-item">
                                <span class="fa-stack fa-4x">
                                <i class="fa fa-circle fa-stack-2x"></i>
                                <i class="fa fa-cloud fa-stack-1x text-primary"></i>
                            </span>
                                <h4>
                                    <strong>Food Analytics</strong>
                                </h4>
                                <p>Analyzing your food preferences.</p>
                                <a href="#" class="btn btn-light">Learn More</a>
                            </div>
                        </div>
                        <div class="col-md-3 col-sm-6">
                            <div class="service-item">
                                <span class="fa-stack fa-4x">
                                <i class="fa fa-circle fa-stack-2x"></i>
                                <i class="fa fa-compass fa-stack-1x text-primary"></i>
                            </span>
                                <h4>
                                    <strong>Recommendation</strong>
                                </h4>
                                <p>Recommend to your taste buds.</p>
                                <a href="#" class="btn btn-light">Learn More</a>
                            </div>
                        </div>
                        <div class="col-md-3 col-sm-6">
                            <div class="service-item">
                                <span class="fa-stack fa-4x">
                                <i class="fa fa-circle fa-stack-2x"></i>
                                <i class="fa fa-flask fa-stack-1x text-primary"></i>
                            </span>
                                <h4>
                                    <strong>End to End System</strong>
                                </h4>
                                <p>Start Hungry, Finish Happy Everyday</p>
                                <a href="#" class="btn btn-light">Learn More</a>
                            </div>
                        </div>
                        <div class="col-md-3 col-sm-6">
                            <div class="service-item">
                                <span class="fa-stack fa-4x">
                                <i class="fa fa-circle fa-stack-2x"></i>
                                <i class="fa fa-shield fa-stack-1x text-primary"></i>
                            </span>
                                <h4>
                                    <strong>Personalized</strong>
                                </h4>
                                <p>Bring our personal menu where ever you go</p>
                                <a href="#" class="btn btn-light">Learn More</a>
                            </div>
                        </div>
                    </div>
                    <!-- /.row (nested) -->
                </div>
                <!-- /.col-lg-10 -->
            </div>
            <!-- /.row -->
        </div>
        <!-- /.container -->
    </section>

    <!-- Callout -->
    <aside class="callout">
        <div class="text-vertical-center" id="front2">
            <h1 style="text-shadow: -1px 0 black, 0 1px black, 1px 0 black, 0 -1px black;">Right Food at the Right Time</h1>
        </div>
    </aside>

    <!-- Portfolio -->
    <section id="portfolio" class="portfolio">
        <div class="container">
            <div class="row">
                <div class="col-lg-10 col-lg-offset-1 text-center">
                    <h2>Discover and Enjoy New Suggestions</h2>
                    <hr class="small">
                         <div class="row">
                            <div class="col-md-3">
                                <div class="portfolio-item">
                                    <div class="card">
                                      <%= image_tag "portfolio-4.jpg", class: "img-fluid" %>
                                      <div class="card-block">
                                      <h9>Nasi Lemak</h9>
                                      <p class="card-text">Nasi lemak is the representation of our country</p>
                                      </div>
                                    </div>
                                </div>
                           </div>
                           <div class="col-md-3">
                                <div class="portfolio-item">
                                    <div class="card">
                                      <%= image_tag "portfolio-1.jpg", class: "img-fluid" %>
                                      <div class="card-block">
                                      <h9>Nasi Ayam Berempah</h9>
                                      <p class="card-text">It was one of the famous delicacies in town!</p>
                                      </div>
                                    </div>
                                </div>
                           </div>
                           <div class="col-md-3">
                                <div class="portfolio-item">
                                    <div class="card">
                                      <%= image_tag "portfolio-2.jpg", class: "img-fluid" %>
